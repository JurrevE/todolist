import { displayProject } from "./displayProject";

export function SubmitBtnFunc() {
    let submitButton = document.getElementById("submitbutton");
    let projectTitle = document.getElementById("projectTitle")


        
        submitButton.addEventListener("click", handleClick);

  

    function handleClick(event) {
        event.preventDefault();

        let titlenameinputvalue = document.getElementById("projectTitle").value;
        console.log(titlenameinputvalue);

        // Assuming dialogProject is defined somewhere in your code
        projectdialog.close();
        projectTitle.value = ""
        displayProject(titlenameinputvalue)
    }
}
